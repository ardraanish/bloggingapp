<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/profile.css">
</head>
<body>
    <div class="container">
        <div class="profile_page">
        <header>
            <div class="header">
                <h1>user blogs</h1>
                <div class="buttons">
                    <button><a href="/login">logout</a></button>
                </div>
             </div>
        </header>
        <hr>
        <div class="profile_area">
            <div class="blog">
                <% if (locals.post) { %>
                    <% post.forEach(e => { %>
                       <li class="li">
                           
                      <span class="title"><%= e.title %></span>
                      <div class="edit_dlt">
                       <a href="/admin/post/<%= e._id %>"><button class="edit"  id="edit_btn">view</button></a>
                      </div>
                   
                   </li>
                    <% }) %>
                   <% } %>
            </div>
        </div>
    </div>
    </div>

    
<!-- <script src="/js/js.js"></script> -->
</body>
</html>